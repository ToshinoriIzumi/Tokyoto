<h1>検索結果</h1>
<table>
  <thead>
    <tr>
      <th>制度名</th>
      <th>内容</th>
      <th>申請方法</th>
      <th>申請締切</th>
      <th>URL</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |support| %>
      <tr>
        <td><%= support.support_name %></td>
        <td><%= support.content %></td>
        <td><%= support.application_method %></td>
        <td><%= support.application_limit %></td>
        <td><%= support.application_limit %></td>
        <td><%= support.url %></td>
        <td><%= link_to '詳細', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Top', users_path %>